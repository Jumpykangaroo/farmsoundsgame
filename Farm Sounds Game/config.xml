<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.elizabeth.farmsoundsgame" version="1.0.0">
    <name>Farm Sounds</name>
    <description>A game for the blind and visually impaired children.</description>
    <author href="http://phonegap.com" email="support@phonegap.com">Elizabeth Peddell</author>
    <content src="index.html"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="android-minSdkVersion" value="14"/>
    <icon src="icon.png"/>
    <platform name="android">
        <icon src="images/PNGs/background" density="ldpi"/>
        <icon src="images/PNGs/background" density="mdpi"/>
        <icon src="images/PNGs/background" density="hdpi"/>
        <icon src="images/PNGs/background" density="xhdpi"/>
        <icon src="images/PNGs/background" density="xxhdpi"/>
        <icon src="images/PNGs/background" density="xxxhdpi"/>
        <splash src="images/PNGs/background" density="land-ldpi"/>
        <splash src="images/PNGs/background" density="land-mdpi"/>
        <splash src="images/PNGs/background" density="land-hdpi"/>
        <splash src="images/PNGs/background" density="land-xhdpi"/>
        <splash src="images/PNGs/background" density="land-xxhdpi"/>
        <splash src="images/PNGs/background" density="land-xxxhdpi"/>
        <splash src="images/PNGs/background" density="port-ldpi"/>
        <splash src="images/PNGs/background" density="port-mdpi"/>
        <splash src="images/PNGs/background" density="port-hdpi"/>
        <splash src="images/PNGs/background" density="port-xhdpi"/>
        <splash src="images/PNGs/background" density="port-xxhdpi"/>
        <splash src="images/PNGs/background" density="port-xxxhdpi"/>
    </platform>
    <platform name="ios">
        <icon src="images/PNGs/background" platform="ios" width="57" height="57"/>
        <icon src="images/PNGs/background" platform="ios" width="114" height="114"/>
        <icon src="images/PNGs/background" platform="ios" width="40" height="40"/>
        <icon src="images/PNGs/background" platform="ios" width="80" height="80"/>
        <icon src="images/PNGs/background" platform="ios" width="50" height="50"/>
        <icon src="images/PNGs/background" platform="ios" width="100" height="100"/>
        <icon src="images/PNGs/background" platform="ios" width="60" height="60"/>
        <icon src="images/PNGs/background" platform="ios" width="120" height="120"/>
        <icon src="images/PNGs/background" platform="ios" width="180" height="180"/>
        <icon src="images/PNGs/background" platform="ios" width="72" height="72"/>
        <icon src="images/PNGs/background" platform="ios" width="144" height="144"/>
        <icon src="images/PNGs/background" platform="ios" width="76" height="76"/>
        <icon src="images/PNGs/background" platform="ios" width="152" height="152"/>
        <icon src="images/PNGs/background" platform="ios" width="29" height="29"/>
        <icon src="images/PNGs/background" platform="ios" width="58" height="58"/>
        <icon src="images/PNGs/background" platform="ios" width="87" height="87"/>
        <splash src="images/PNGs/background" platform="ios" width="640" height="1136"/>
        <splash src="images/PNGs/background" platform="ios" width="750" height="1334"/>
        <splash src="images/PNGs/background" platform="ios" width="1242" height="2208"/>
        <splash src="images/PNGs/background" platform="ios" width="2208" height="1242"/>
        <splash src="images/PNGs/background" platform="ios" width="2048" height="1536"/>
        <splash src="images/PNGs/background" platform="ios" width="1024" height="768"/>
        <splash src="images/PNGs/background" platform="ios" width="1536" height="2048"/>
        <splash src="images/PNGs/background" platform="ios" width="768" height="1024"/>
        <splash src="images/PNGs/background" platform="ios" width="640" height="960"/>
        <splash src="images/PNGs/background" platform="ios" width="320" height="480"/>
    </platform>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>
